{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "sveltekit",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { 
          "key": "Content-Security-Policy", 
          "value": "frame-ancestors 'self' https://mishan3.xyz https://www.mishan3.xyz;" 
        },
        { 
          "key": "X-Frame-Options", 
          "value": "ALLOWALL" 
        }
      ]
    }
  ]
}
